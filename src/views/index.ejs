  <div class="col">
    <form action="/contact" method="POST">
      <div class="row mt-2">
        <div class="col-3 text-end contactText"><label for="contactName"><h4>Name</h4></label></div> <div class="col-1"></div>
        <div class="col text-start contactInputs"><input id="contactName" placeholder="John Doe" type="text" name="name" required="true" autocomplete="additional-name"></div>
      </div>
      <div class="row mt-2">
        <div class="col-3 text-end contactText"><label for="contactEmail"><h4>Email</h4></label></div> <div class="col-1"></div>
        <div class="col text-start contactInputs"><input id="contactEmail" placeholder="johndoe@example.com" type="text" name="email" required="true" autocomplete="additional-name"><br></div>
      </div>
      <div class="row mt-2">
        <div class="col-3 text-end contactText"><label for="contactSubject"><h4>Subject</h4></label></div> <div class="col-1"></div>
        <div class="col text-start contactInputs"><input id="contactSubject" placeholder="Subject Field" type="text" name="subject" required="true"><br></div>
      </div>
      <div class="row mt-2">
        <div class="col-3 text-end contactText"><label for="contactContent"><h4>Content</h4></label></div> <div class="col-1"></div>
        <div class="col text-start contactInputs"><textarea class="" id="contactContent" placeholder="Main Content Section" type="text" name="content" required="true"></textarea><br></div>
      </div>
      <div class="col text-center"><button id="contactSubmit">Submit</button><input class="resetButton ms-5" type="reset" value="Reset"></div>
    </form>
  </div>
